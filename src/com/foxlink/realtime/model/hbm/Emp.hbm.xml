<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.foxlink.realtime.model.Emp" table="EMP">
		<id name="EMPNO" type="string">      <!-- 主键 -->
			<column name="EMPNO" length="10"/>
			<generator class="assigned"/><!-- Hibernate的主键生成策略,assigned:主键由外部程序负责生成，在 save() 之前指定。 -->
		</id>
				
		<property name="NAME" type="string">
			<column name="NAME" length="20" not-null="true" unique="false"/>
		</property>
	
		<property name="DEPTNO" type="string">
			<column name="DEPTNO" length="6" not-null="true" unique="false"/>
		</property>
	</class>
</hibernate-mapping>